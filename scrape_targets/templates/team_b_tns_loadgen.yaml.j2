scrape_configs:
- job_name: team_b_tns_loadgen
  static_configs:
    - targets: ['team-b:8001']
      labels:
        origin: 'team-b'
        process: 'tns_loadgen'
remote_write:
- url: {{ secrets.remote_write_url }}
  basic_auth:
    username: {{ secrets.remote_write_username }}
    password: {{ secrets.remote_write_password }}
