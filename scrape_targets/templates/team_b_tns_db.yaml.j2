scrape_configs:
- job_name: team_b_tns_db
  static_configs:
    - targets: ['team-b:8000']
      labels:
        origin: 'team-b'
        process: 'tns_db'
remote_write:
- url: {{ secrets.remote_write_url }}
  basic_auth:
    username: {{ secrets.remote_write_username }}
    password: {{ secrets.remote_write_password }}
