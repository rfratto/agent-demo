scrape_configs:
- job_name: team_a_tns_app
  static_configs:
    - targets: ['team-a:8080']
      labels:
        origin: 'team-a'
        process: 'tns_app'
remote_write:
- url: {{ secrets.remote_write_url }}
  basic_auth:
    username: {{ secrets.remote_write_username }}
    password: {{ secrets.remote_write_password }}
